@model List<Fiap.Exemplo01.MVC.Web.Models.Cliente>
@{
    ViewBag.Title = "Listar";
}

<h2>Listar</h2>

<table class="table">
    <thead>
        <tr>
            <th>Nome</th>
            <th>Idade</th>
            <th>Crédito</th>
            <th>Estado Civil</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Nome</td>
                <td>@item.Idade</td>
                <td>@item.Credito</td>
                <td>@item.EstadoCivil</td>
            </tr>
        }
    </tbody>
</table>

